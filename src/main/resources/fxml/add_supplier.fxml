<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="420.0" prefWidth="712.0" style="-fx-background-image: #87CEEB;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soft1841.sm.controller.AddSupplierController">
    <HBox alignment="CENTER" minHeight="50" styleClass="blue-theme" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
        <Label styleClass="btn3-basic" text="新增供应商" />
    </HBox>
        <HBox spacing="10" />

    <Button onAction="#addSupplier" styleClass="btn4-basic" text="确认" AnchorPane.bottomAnchor="10" AnchorPane.leftAnchor="180" AnchorPane.rightAnchor="180" />
         <TextField fx:id="supplierAddress" layoutX="255.0" layoutY="117.0" promptText="供应商地址" style="-fx-background-color: #87CEEB;" />
         <TextField fx:id="supplierPhone" layoutX="255.0" layoutY="180.0" prefWidth="290" promptText="联系方式" style="-fx-background-color: #87CEEB;" />
         <TextField fx:id="linkMen" layoutX="255.0" layoutY="247.0" prefWidth="290" promptText="称呼" />
         <TextField fx:id="supplierName" layoutX="255.0" layoutY="60.0" promptText="供应商名称" style="-fx-background-color: #87CEEB;" />

        <padding>
            <Insets bottom="10" left="50" right="50" top="10" />
        </padding>
</AnchorPane>
