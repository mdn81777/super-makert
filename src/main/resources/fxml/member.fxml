<!--<?xml version="1.0" encoding="UTF-8"?>-->
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<!--<?xml version="1.0" encoding="UTF-8"?>-->
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane prefHeight="335.0" prefWidth="603.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soft1841.sm.controller.MemberController">
    <HBox spacing="20" AnchorPane.leftAnchor="220" AnchorPane.topAnchor="17" prefWidth="150">
        <Button text="添加会员" onAction="#newMemberStage" styleClass="green-theme"/>
    </HBox>
    <TableView fx:id="memberTable" prefHeight="600" prefWidth="1490.0" AnchorPane.leftAnchor="220" AnchorPane.topAnchor="80">
        <columns>
            <TableColumn text="ID">
                <cellValueFactory>
                    <PropertyValueFactory property="id"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="会员姓名">
                <cellValueFactory>
                    <PropertyValueFactory property="name"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="会员地址">
                <cellValueFactory>
                    <PropertyValueFactory property="address"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="联系电话">
                <cellValueFactory>
                    <PropertyValueFactory property="phone"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="会员积分">
                <cellValueFactory>
                    <PropertyValueFactory property="integral"/>
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
</AnchorPane>