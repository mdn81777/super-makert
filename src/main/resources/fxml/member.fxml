<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane prefHeight="335.0" prefWidth="603.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soft1841.sm.controller.SupplierController">
    <HBox spacing="20" AnchorPane.leftAnchor="20" AnchorPane.topAnchor="10">
        <Button text="添加会员" />
    </HBox>
    <TableView fx:id="MemberTable" prefHeight="262.0" prefWidth="566.0" AnchorPane.leftAnchor="20" AnchorPane.topAnchor="50">
        <columns>
            <TableColumn text="会员ID">
                <cellValueFactory>
                    <PropertyValueFactory property="id"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="会员姓名">
                <cellValueFactory>
                    <PropertyValueFactory property="member_name"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="会员地址">
                <cellValueFactory>
                    <PropertyValueFactory property="member_address"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="会员联系方式">
                <cellValueFactory>
                    <PropertyValueFactory property="member_phone"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="会员积分">
                <cellValueFactory>
                    <PropertyValueFactory property="member_integral"/>
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
</AnchorPane>