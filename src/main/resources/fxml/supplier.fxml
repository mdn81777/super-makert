<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane prefHeight="335.0" prefWidth="603.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.soft1841.sm.controller.SupplierController">
<HBox spacing="20" AnchorPane.leftAnchor="20" AnchorPane.topAnchor="10">
    <Button text="添加供应商" onAction="#listSupplier" />
</HBox>
<TableView fx:id="supplierTable" prefHeight="262.0" prefWidth="566.0" AnchorPane.leftAnchor="20" AnchorPane.topAnchor="50">
    <columns>
        <TableColumn text="ID">
            <cellValueFactory>
                <PropertyValueFactory property="id"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="供应商名称">
            <cellValueFactory>
                <PropertyValueFactory property="supplier_name"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="供应商地址">
            <cellValueFactory>
                <PropertyValueFactory property="supplier_address"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="供应商联系方式">
            <cellValueFactory>
                <PropertyValueFactory property="supplier_phone"/>
            </cellValueFactory>
        </TableColumn>
        <TableColumn text="供应商称呼">
            <cellValueFactory>
                <PropertyValueFactory property="linkman"/>
            </cellValueFactory>
        </TableColumn>
    </columns>
</TableView>
</AnchorPane>
